{
  "extends": "eslint:recommended",
  "parser-options": {
    "ecmaVersion": 5,
    "sourceType": "module",
    "ecmaFeatures": {
      "globalReturn ": false,
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "phantomjs": true
  },
  "rules": {
    "quotes": ["error", "single"]
  }
}